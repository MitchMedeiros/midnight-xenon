    "\n",
    "df = px.data.iris()\n",
    "fig = px.scatter(\n",
    "    df,\n",
    "    x=\"sepal_width\",\n",
    "    y=\"sepal_length\",\n",
    "    color=\"species\",\n",
    "    size=\"petal_length\",\n",
    "    hover_data=[\"petal_width\"],\n",
    ")\n",
    "fig.show()"
