{
	"name": "Dark Xenon",
	"semanticHighlighting": true,
	"semanticTokenColors": {
		// "class.decorator": {
		// 	"foreground": "#D6D6D6",
		// }
	},
	"tokenColors": [
		{
			"name": "Class",
			"scope": [
				"class",
				"entity.name.type.class",
				"entity.name.class",
				"entity.other.inherited-class",
				"support.class",
				"storage.type.annotation.java",
			],
			"settings": {
				"foreground": "#30E5BE"
			}
		},
		{
			"name": "Function/Method",
			"scope": [
				"function",
				"method",
				"entity.name.function",
				"entity.name.method",
				"support.function",
				"support.variable.magic.python",
				"support.function.magic.python",
				"storage.type.tag-handle.yaml",
			],
			"settings": {
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"entity.name.variable",
				"variable.parameter",
				"variable.other.property",
				"variable.other.readwrite",
				"variable.other.constant",
				"support.variable.magic.python",
				"variable.stable",
				"variable.volatile",
				"variable.function",
				"variable.function.constructor",
				"support.type.property-name.json",
				"entity.other.attribute-name.html",
				"entity.other.attribute-name.xml",
				"entity.other.attribute-name.namespace.xml",
				"entity.other.attribute-name.localname.xml",
				"entity.name.tag.yaml",
				"keyword.other.definition.ini",
				"variable.other.env",
			],
			"settings": {
				"foreground": "#88D5FF"
			}
		},
		{
			"name": "Primitive/Constant/escape character",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"storage.type.primitive.java",
			],
			"settings": {
				"foreground": "#AEFF7F"
			}
		},
		{
			"name": "Library/Module",
			"scope": [
				"entity.name.namespace",
				"entity.name.import",
				"meta.import",
			],
			"settings": {
				"foreground": "#8F97FF"
			}
		},
		{
			"name": "Flow/Import/Other Keyword",
			"scope": [
				"keyword.control.flow",
				"keyword.operator.logical",
				"keyword.other.import",
				"keyword.control.import.python",
				"keyword.control.java",
				"keyword.control.throw",
				"keyword.other.package.java",
				"keyword.other.env",
			],
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"name": "Definition Keyword",
			"scope": [
				"storage.type",
				"keyword.declaration",
				"keyword.declaration.scala",
				"keyword.declaration.stable.scala",
				"keyword.declaration.volatile.scala",
				"keyword.interpolation.scala",
				"punctuation.definition.template-expression.begin.scala",
				"entity.name.tag",
				"storage.modifier.extends.java",
				"storage.modifier.java",
				"keyword.control.new.java",
				"storage.modifier.other",
				"keyword.other.doctype.xml",
			],
			"settings": {
				"foreground": "#22ADF8"
			}
		},
		{
			"name": "Logic Keyword",
			"scope": [
				"keyword.operator.logical",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#22ADF8"
			}
		},
		{
			"name": "Self/This Keyword",
			"scope": [
				"variable.parameter.function.language.special.self",
				"variable.language.scala",
				"variable.language.this.java",
			],
			"settings": {
				"foreground": "#B9CEDA",
			}
		},
		{
			"name": "Number",
			"scope": [
				"number",
				"constant.numeric",
			],
			"settings": {
				"foreground": "#B5CEA8"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string",
				"comment.block.documentation.scala",
				"string.quoted.docstring",
				"comment.block.javadoc.java",
				"entity.name.section.group-title.ini",
				"markup.underline.link"
			],
			"settings": {
				"foreground": "#EEBC70"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment.line.number-sign.python",
				"punctuation.definition.comment.python",
				"punctuation.definition.comment.json.comments",
				"comment.line.double-slash",
				"comment.block.xml",
				"comment.line.number-sign.yaml",
				"comment.line.number-sign",
				"comment.line.semicolon.ini",
			],
			"settings": {
				"foreground": "#60737D"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.section",
				"punctuation.separator.key-value.ini"
			],
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"name": "Type",
			"scope": [
				"entity.name.type",
				"support.type",
			],
			"settings": {
				"foreground": "#30E5BE"
			}
		},
		{
			"name": "Single Braces",
			"scope": [
				"punctuation.definition.dict.begin.python",
				"punctuation.definition.dict.end.python",
				"keyword.other.template.begin.env",
				"keyword.other.template.end.env",
			],
			"settings": {
				"foreground": "#F9D849"
			}
		},
		{
			"name": "Off-white text",
			"scope": [
				"punctuation.definition.tag",
			],
			"settings": {
				"foreground": "#a6b3b1e5",
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Plain Text",
			"scope": [
				"meta.paragraph.markdown",
				"markup.table",
				"punctuation.definition.list_item.markdown",
				"text.html",
				"text.xml",
			],
			"settings": {
				"foreground": "#D5E4E4"
			}
		},
		{
			"name": "Markup - Heading",
			"scope": [
				"markup.heading",
				"markdown.heading",
			],
			"settings": {
				"foreground": "#88D5FF"
			}
		},
		{
			"name": "Link Description",
			"scope": [
				"string.other.link.description.markdown",
				"string.other.link.title.markdown",
				"constant.other.reference.link.markdown",
			],
			"settings": {
				"foreground": "#2EDCB7"
			}
		},
		{
			"name": "Markup - List Punctuation",
			"scope": [
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.quote.begin.markdown",
				"punctuation.definition.table.markdown",
				"punctuation.separator.table.markdown",
			],
			"settings": {
				"foreground": "#8F97FF"
			}
		},
		{
			"name": "Code/Quotes",
			"scope": [
				"markup.quote",
				"markup.quote.markdown meta.paragraph.markdown",
				"markup.raw.block",
				"markup.fenced_code.block",
			],
			"settings": {
				"foreground": "#B5CEA8"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Markup - Bold-Italic",
			"scope": [
				"markup.bold markup.italic",
			],
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Markup - strikethrough",
			"scope": [
				"markup.strikethrough",
			],
			"settings": {
				"fontStyle": "strikethrough",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Markup - Strikethrough-Italic",
			"scope": [
				"markup.strikethrough markup.italic",
			],
			"settings": {
				"fontStyle": "strikethrough italic",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Markup - Strikethrough-Bold",
			"scope": [
				"markup.strikethrough markup.bold",
			],
			"settings": {
				"fontStyle": "strikethrough bold",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Markup - Underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#FFFFA8"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted",
				"markup.inserted.git_gutter",
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted",
				"markup.deleted.git_gutter",
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed",
				"markup.changed.git_gutter",
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		// CSS
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
	],
